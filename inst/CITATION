citHeader("To cite fastco in a publication use:")

citEntry(
  entry = "Misc",
  title = "{fastco}: Fast Correlation, Covariance, and Cosine Similarity",
  author = personList(as.person("Drew Schmidt")),
  year = "2016",
  note = "{R} Package",
  url = "http://cran.r-project.org/package=fastco",
  textVersion = "Schmidt, D. (2016) fastco: Fast Correlation, Covariance, and Cosine Similarity. R Package, URL http://cran.r-project.org/package=fastco",
  key = "fastco2016"
)

citEntry(
  entry = "Manual",
  title = "Introducing {fastco}: Fast Covariance, Correlation, and Cosine Operations",
  author = personList(as.person("Drew Schmidt")),
  year = "2016",
  note = "{R} Vignette",
  url = "http://cran.r-project.org/package=fastco",
  textVersion = "Schmidt, D. (2016) Introducing fastco: Fast Covariance, Correlation, and Cosine Operations. R Vignette, URL http://cran.r-project.org/package=fastco",
  key = "fastco2016guide"
)

citEntry(
  entry = "Manual",
  title = "Algorithms and Benchmarks for the {fastco} Package",
  author = personList(as.person("Drew Schmidt")),
  year = "2016",
  note = "{R} Vignette",
  url = "http://cran.r-project.org/package=fastco",
  textVersion = "Schmidt, D. (2016) Algorithms and Benchmarks for the {fastco} Package. R Vignette, URL http://cran.r-project.org/package=fastco",
  key = "fastco2016algos"
)
